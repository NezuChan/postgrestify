    "@types/pg": "^8.15.4",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.17.0",
    "dotenv": "^16.6.1",
    "drizzle-kit": "^0.31.4",
    "eslint": "^8.57.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@nezuchan/schema": "^0.1.4",
