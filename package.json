    "@types/pg": "^8.11.6",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.14.1",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.21.4",
    "eslint": "^8.57.0",
    "rimraf": "^5.0.7",
    "typescript": "^5.5.2"
  },
  "dependencies": {
    "@nezuchan/schema": "^0.1.4",
